  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <JapaneseText>やあ、帰ってきたのか…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>12</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ほんの一月会ってないだけなのに
ひどく懐かしいね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>12</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ディセンダーであろうと、
どうだろうと君は我々の仲間だよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>12</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>では、今後ともよろしく</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>12</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
