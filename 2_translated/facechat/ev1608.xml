<SceneText>
  <Speakers>
    <Section>Speaker</Section>
    <Entry>
      <JapaneseText>カノンノ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>50</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <JapaneseText>がんばってるね、
○○</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>50</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>お話書けてるかって？
ううん、ちっとも…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>50</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>何を書いたらいいのかなぁって
いい物語が思い浮かばなくて</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>50</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>何だか色々迷っちゃって</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>50</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>書いては破り、書いては破りだよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>50</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>なかなか上手くいかない
もんだなぁ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>50</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
