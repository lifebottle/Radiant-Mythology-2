<SceneText>
  <Speakers>
    <Section>Speaker</Section>
    <Entry>
      <JapaneseText>ゼロス</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>29</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>リフィル</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ロイド</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>24</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <JapaneseText>さあて、リフィル様にロイドく〜ん！
元気ぃ？
俺さま超元気ぃ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>29</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>俺さまの今後の大活躍見ててくれよ〜？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>29</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ええ、あまり期待していないから、
ほどほどの活躍でよろしくてよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>27</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>とほ〜〜〜…、きびし〜よなぁ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>29</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>オレはゼロスの活躍、ちゃんと
期待してるぜ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>24</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ありがと〜〜〜〜￥
さすがハニーだぜ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>29</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>俺さまのみっちり詰まった愛情を
ごろうぜよ、だぜ〜</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>29</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>そういうのは期待してねぇって…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>24</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
