<SceneText>
  <Speakers>
    <Section>Speaker</Section>
    <Entry>
      <JapaneseText>カノンノ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>50</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>パニール</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>53</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <JapaneseText>気がついたんだ、よかった…</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>50</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>カノンノ、どう？
お目覚めになったの？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>53</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>あら、あれ、んまー。
りりしいお坊っちゃまだことー</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>53</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>あら、あれ、んまー。
可愛らしいお嬢さまだことー</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>53</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ご無事で何よりだわ。
ここは、バンエルティア号という
船の中なの</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>53</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>私、カノンノ。
あなたは？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>50</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>○○…、いい名前ね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>50</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>無事で何よりだわ〜。
あなた空から降ってきたの。
ここの甲板にね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>53</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
