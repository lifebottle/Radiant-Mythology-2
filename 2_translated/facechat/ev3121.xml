    <Entry>
      <JapaneseText>ルーティ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <JapaneseText>おかえり、○○</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>おかえりなさい</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>9</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>また会いに来てくれて、
うれしいよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>やっぱり、ここがいいんでしょ？
みんな、そう言うもの</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>9</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>やっぱり、にぎやかな方がいいよな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>8</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>お互い楽しく仲良くやっていきましょ。
これからも、ね！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>9</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
