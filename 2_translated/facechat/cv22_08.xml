<SceneText>
  <Speakers>
    <Section>Speaker</Section>
    <Entry>
      <JapaneseText>リフィル</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>クロエ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <Id>38</Id>
      <Status>To Do</Status>
    </Entry>
  </Speakers>
  <Strings>
    <Section>Main Text</Section>
    <Entry>
      <JapaneseText>あなたを見て思ったんだけど、
クロエ、あなたカナヅチね？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>27</SpeakerId>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>な、何を根拠に！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>38</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>違って？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>27</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>い、いや…、違わない</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>38</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>そう…。
安心したわ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>27</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>！！
という事は、おまえも…？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>38</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>…お互い水辺では注意しましょう。
どっちかが溺れても
助けてもらえないのだから</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>27</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <JapaneseText>ああ、心得た。
リフィルとは絶対水辺に近寄らない</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>38</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>Unreferenced</Section>
  </Strings>
</SceneText>
